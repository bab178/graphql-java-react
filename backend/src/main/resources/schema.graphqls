type Query {
  bookById(id: ID): Book
  bookList: [Book]!
}

type Mutation {
  addBook(newBook: BookInput!): Book
}

input BookInput {
  bookId: ID!
  name: String
  pageCount: Int
}


type Book {
  id: ID
  name: String
  pageCount: Int
  author: Author
}

type Author {
  firstName: String
  lastName: String
}